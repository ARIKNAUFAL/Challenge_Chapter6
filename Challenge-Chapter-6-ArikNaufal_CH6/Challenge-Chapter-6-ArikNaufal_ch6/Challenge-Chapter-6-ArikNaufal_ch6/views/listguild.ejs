<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <title><%= title %></title>
</head>
<style>
  .card{
    margin-top: 50px;
  }
  .navbar-brand img{
  width: 100px;
  }
</style>
  <body> 
    <nav class="navbar navbar-dark bg-dark">
      <div class="container-fluid">
        <a class="navbar-brand" href="/"><img src="./images/logo.png" alt=""></a>
        <ul class=" d-flex justify-content-evenly" style="margin-bottom: 0px;
        padding-top: 10px">
          <li class="nav-item">
            <a class="nav-link active text-white" aria-current="page" href="/list">Player</a>
          </li>
          <li class="nav-item">
            <a class="nav-link text-white" href="/guild">Guild</a>
          </li>
          <li class="nav-item">
            <a class="nav-link text-white" href="/server">Server</a>
          </li>
        <form class="d-flex">
          <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
          <button class="btn btn-warning" type="submit">Search</button>
        </form>
      </div>
    </nav>
     
    <div class="container">
      <h2>GUILD LIST</h2>

      <% for( let i = 1; i < list.length; i++ ) { %>
        <div class="card" style="width: 20rem;">
          <div class="title" style="width: 100%;">
            <div class="inner-title mx-auto" style="width: 60%;">
              <h2 class="text-center"><%= list[i].name %></h2>
            </div>
          </div>
          <div class="card-body">
            <p class="card-text">win: <span class="fw-bold"><%= list[i].win %></span> </p>
            <p class="card-text">lose: <span class="fw-bold"><%= list[i].lose %></span></p>
            <a href="/guild/member/<%= list[i].name%>" class="btn btn-primary">detail</a>
          </div>
        </div>
      <% } %>


    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
  </body>
</html>
